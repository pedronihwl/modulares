generator client {
  provider = "prisma-client"
  output   = "./generated"
}

datasource db {
  provider = "postgresql"
}

model Video {
  id           String   @id
  title        String
  description  String
  url          String
  sizeInKb     Int
  duration     Int
  thumbnailUrl String?
  createdAt    DateTime
  updatedAt    DateTime
}
